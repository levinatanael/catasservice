<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
	<appSettings>
		<!-- Endpoint API dos Gatos-->
		<add key="EndpointCatAPI" value="https://api.thecatapi.com/v1/" />
		<!--Raças dos gatos-->
		<add key="EndpointCatAPIBreeds" value="breeds" />
		<!--Buscar informações e imagem de determinada raça-->
		<add key="EndpointCatAPISearchBreed" value="images/search?breed_ids={0}" />
	</appSettings>
</configuration>
